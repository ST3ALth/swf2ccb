/*This script is used to export all the movieClip symbols and to identify all the shape/shape tweens that need to be changedso the outputted swf can be used with swf2Cocos*/var libraryItems = fl.getDocumentDOM().library.items;var breakOut = false;fl.trace("The following won't work with swf2Cocos. Please convert the mentioned flash shapes to symbols and change the shape tweens to either motion tweens or removed them:");for each (var item in libraryItems) {	if (item.itemType == "movie clip") {				var itemTimeline = item.timeline;				for each (var l in itemTimeline.layers) {			for (var f = 0; f < l.frames.length; f++) {				if (l.frames[f].tweenType == "shape") {					fl.trace("shape tween in the \"" + l.name + "\" layer of " + item.name);					break;				}			}		}				var baseClass = item.linkageBaseClass;				if (baseClass == "flash.display.MovieClip" || baseClass == undefined || baseClass == "" && !item.linkageImportForRS) {    		item.linkageExportForAS = true;    		item.linkageExportInFirstFrame = true;		}				var childTimeline = item.timeline;		var shapes = [];		var allShapes = true;		var graphics = [];				for each (var layer in childTimeline.layers) {			if (layer.frames != null) {				for each (var f in layer.frames) {					if (f.elements != null) {						for each (var element in f.elements) {							if (element.elementType == "shape") {								if (shapes.indexOf(element) == -1) {									shapes.push(element);								}							}							else {								allShapes = false;							}														if (element.elementType == "instance" && element.symbolType == "graphic") {								if (graphics.indexOf(element) == -1) {									graphics.push(element);								}							}						}					}				}			}		}				if (!allShapes) {			breakOut = true;		}				if (breakOut) {			for each (var layer in childTimeline.layers) {				if (layer.frames != null) {					for (var f = 0; f < layer.frames.length; f++) {						if (layer.frames[f].elements != null) {							for each (var element in layer.frames[f].elements) {								if (element.elementType == "shape") {									if (shapes.indexOf(element) != -1) {										fl.trace("flash shape in the \"" + layer.name + "\" layer of " + item.name + " at frame " + (f+1) + ". Convert to Symbol.");										shapes.splice(shapes.indexOf(element), 1);									}									allSymbols = false;								}																if (element.elementType == "instance" && element.symbolType == "graphic") {									if (graphics.indexOf(element) != -1) {										fl.trace("graphic in the \"" + layer.name + "\" layer of " + item.name + " at frame " + (f+1) + ". Convert to Symbol.");										graphics.splice(graphics.indexOf(element), 1);									}								}							}						}					}				}			}			breakOut = false;		}	}}